    "postinstall": "husky install",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "start": "webpack serve --mode development",
    "start:swc": "webpack serve --mode development --config ./webpack-swc.config.cjs",
    "build": "webpack",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "profile:dev": "webpack --mode development --profile",
    "profile:prod": "webpack --mode production --profile",
    "vite": "vite",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:swc": "jest --config=swc-jest.config.cjs",
    "test:swc:cov": "jest --config=swc-jest.config.cjs --coverage",
    "nginx": "docker compose -f ./docs/docker/webservers/nginx/docker-compose.yml up --build --detach --remove-orphans",
    "nginx:stop": "docker compose -f ./docs/docker/webservers/nginx/docker-compose.yml down",
    "nginx:restart": "docker compose -f ./docs/docker/webservers/nginx/docker-compose.yml restart",
    "httpd": "docker compose -f ./docs/docker/webservers/httpd/docker-compose.yml up --build --detach --remove-orphans",
    "httpd:stop": "docker compose -f ./docs/docker/webservers/httpd/docker-compose.yml down",
